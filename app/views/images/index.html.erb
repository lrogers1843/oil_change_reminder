<h1>Images</h1>
<table>
  <thead>
    <tr>
      <th>Time stamp</th>
      <th>Odometer reading</th>
      <th>Oil change</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @images.each do |image| %>
      <tr>
        <td><%= image.time_stamp %></td>
        <td><%= image.odometer_reading %></td>
        <td><%= image.oil_change %></td>
        <td><%= link_to 'View Image', image.picture %></td>
        <td><%= link_to 'Show', image %></td>
        <td><%= link_to 'Edit', edit_image_path(image) %></td>
        <td><%= link_to 'Destroy', image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'New Image', new_image_path %>
